{
  "functions": {
    "v3_demo_server.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "production_app/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "v3_demo_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "v3_demo_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "production_app/frontend/build/$1"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  },
  "installCommand": "cd production_app/frontend && npm install",
  "buildCommand": "cd production_app/frontend && npm run build",
  "outputDirectory": "production_app/frontend/build"
}